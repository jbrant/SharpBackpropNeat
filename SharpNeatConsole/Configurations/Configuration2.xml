<?xml version="1.0" encoding="utf-8"?>

<Config>
  <PopulationSize>150</PopulationSize>
  <SpecieCount>10</SpecieCount>
  <ActivationCppn>
    <Scheme>CyclicFixedIters</Scheme>
    <Iters>4</Iters>
  </ActivationCppn>
  <Activation>
    <Scheme>Acyclic</Scheme>
  </Activation>
  <ComplexityRegulationStrategy>Relative</ComplexityRegulationStrategy>
  <ComplexityThreshold>10</ComplexityThreshold>
  <LengthCppnInput>true</LengthCppnInput>
  <Resolution>28</Resolution>
  <ResolutionReductionPerSide>4</ResolutionReductionPerSide>
  <GenomeConfig>
    <InitialConnectionProportion>.5</InitialConnectionProportion>
    <WeightMutationProbability>.9</WeightMutationProbability>
    <AddConnnectionProbability>0.1</AddConnnectionProbability>
    <AddNodeProbability>.1</AddNodeProbability>
  </GenomeConfig>
  <TrainingImages>.\ImageData\Number1Samples.data</TrainingImages>
  <NumImageSamples>100</NumImageSamples>
  <MaxGenerations>50</MaxGenerations>
  <LearningRate>1</LearningRate>
  <NumBackpropagationIterations>50</NumBackpropagationIterations>
  <TrainingSampleProportion>0.8</TrainingSampleProportion>
  <Description>
    CAP 6616, generating an Autoencoder with HyperNEAT
  </Description>
</Config>